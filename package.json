  "version": "1.2.0",
  "description": "RFID NDC123 Tag for Laboratory PC - Client Application",
  "main": "client/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server/server.js",
    "start-client": "electron client/main.js",
    "dev": "electron client/main.js --dev",
    "build": "electron-builder",
    "build-installer": "electron-builder --win",
    "start-update-server": "node update-server.js",
    "publish": "electron-builder --publish=always"
  },
  "keywords": [
    "rfid",
    "laboratory",
    "pc",
    "monitoring"
  ],
  "author": "Lawrence Heras",
  "license": "ISC",
  "type": "commonjs",
