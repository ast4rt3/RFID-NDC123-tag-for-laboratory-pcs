  "devDependencies": {
    "electron": "^37.2.3",
    "electron-builder": "^24.9.1",
    "electron-updater": "^6.1.7"
  },
  "build": {
    "appId": "com.rfid.ndc123.client",
    "productName": "RFID NDC123 Client",
    "directories": {
      "output": "dist"
    },
    "files": [
      "client/**/*",
      "node_modules/**/*",
      "config.json"
    ],
    "win": {
      "icon": "client/assets/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
      "nsis": {
        "oneClick": false,
        "allowToChangeInstallationDirectory": true,
        "createDesktopShortcut": true,
        "createStartMenuShortcut": true,
        "shortcutName": "Darwin",
        "installerIcon": "client/assets/icon.ico",
        "uninstallerIcon": "client/assets/icon.ico"
      },
      "publish": {
        "provider": "github",
        "owner": "yourusername",
        "repo": "your-repo"
      },
    "asarUnpack": [
      "client/pc-logger.js",
      "client/config.js",
      "client/*.node",
      "config.json",
      "node_modules/**",
